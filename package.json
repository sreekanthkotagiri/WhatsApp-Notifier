{
  "name": "whatsapp-auto-notifier",
  "version": "1.0.0",
  "description": "A NestJS application for automating WhatsApp notifications.",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.6.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "pg": "^8.18.0",
    "serverless-http": "^2.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "typeorm": "^0.3.28",
    "typescript": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^27.0.0",
    "@types/supertest": "^2.0.12",
    "jest": "^27.0.0",
    "supertest": "^6.3.3",
    "ts-jest": "^27.0.0",
    "ts-loader": "^9.0.0"
    ,"@types/aws-lambda": "^8.10.114"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.ts",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "license": "MIT"
}
